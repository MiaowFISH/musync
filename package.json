{
  "name": "musync",
  "version": "1.0.0",
  "description": "网易云音乐歌单同步工具 - Sync NetEase Cloud Music playlists to local",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "musync": "dist/index.js"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --minify",
    "test": "bun test",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "netease",
    "music",
    "playlist",
    "sync",
    "download",
    "cli"
  ],
  "author": "MiaowFISH <neko-project@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "@neteasecloudmusicapienhanced/api": "^4.29.20",
    "music-metadata": "^10.6.4",
    "commander": "^13.1.0",
    "p-limit": "^6.2.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "^22.19.3",
    "typescript": "^5.9.2"
  }
}
